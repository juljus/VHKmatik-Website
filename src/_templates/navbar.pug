.nav-content
    for i in self.nav
        if i.url
            if i.title == "VHK Matik"
                div.navbar-home-logo-div
                    a(href=i.url)
                        img.navbar-home-logo(src="/_assets/VHK_matik_logo.png", alt="VHK_matik_logo")
                    a(href=i.url)=i.title
            else
                div.nav-content-div
                    a(href=i.url)=i.title
        else
            div.nav-content-div
                ul.dropdown teams
                    div
                        for j in i 
                            li
                                a(href=j.url)=j.title

.nav-lang
    each p, locale in self.otherLocalePaths
        a(href= p)= locale
